
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/ahb/18100000.wmac'
	option htmode 'HT20'

config wifi-iface 'wifinet1'
	option ssid 'AusOcean-Client'
	option encryption 'psk2'
	option device 'radio0'
	option mode 'ap'
	option network 'lan'
	option key 'aus0cean'

config wifi-iface 'wifinet2'
	option key 'aus0cean'
	option ssid 'AusOcean-AP-OpenWRT'
	option device 'radio0'
	option mode 'sta'
	option bssid 'C0:C9:E3:9A:BB:D6'
	option network 'wwan'
	option encryption 'psk2'

