
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/ahb/18100000.wmac'
	option htmode 'HT20'

config wifi-iface 'wifinet0'
	option ssid 'AusOcean-AP-OpenWRT'
	option encryption 'psk2'
	option device 'radio0'
	option mode 'ap'
	option key 'aus0cean'
	option network 'lan'

